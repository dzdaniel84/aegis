{
  "manifest_version": 2,
  "name": "Aegis",
  "description": "This extension records Google Searches as of right now",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
  	"tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [{
  	  "matches": ["http://*/*", "hptts://*/*"],
  	  "js": ["contentscript.js"]
  }],
  "background": {
  	"scripts": ["background.js"]
  }
}