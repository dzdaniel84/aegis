{
  "manifest_version": 2,
  "name": "Aegis",
  "description": "This extension monitors users' searches to keep track of mental health.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*/*",
  	"tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
  	"scripts": ["js/bg.js"]
  }
}